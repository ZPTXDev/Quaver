{
  "name": "quaver",
  "version": "7.0.0-next.1",
  "type": "module",
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "@discordjs/rest": "^1.7.1",
    "@jeve/lyrics-finder": "^1.0.1",
    "@keyv/sqlite": "^3.6.7",
    "@lavaclient/plugin-effects": "^1.0.0-rc.2",
    "@lavaclient/plugin-queue": "^0.0.1",
    "@lavaclient/spotify": "^3.1.0",
    "@napi-rs/pinyin": "^1.7.3",
    "@zptxdev/zptx-lib": "^1.1.0",
    "crypto-js": "^4.2.0",
    "discord-api-types": "^0.37.73",
    "discord.js": "^14.14.1",
    "express": "^4.18.3",
    "genversion": "^3.2.0",
    "hangul-romanization": "^1.0.1",
    "keyv": "^4.5.4",
    "kuroshiro": "1.2.0",
    "kuroshiro-analyzer-kuromoji": "^1.1.0",
    "lavaclient": "^5.0.0-rc.2",
    "lavalink-protocol": "^1.0.1",
    "lodash-es": "^4.17.21",
    "patch-package": "^6.5.1",
    "socket.io": "^4.7.4",
    "wanakana": "^5.3.1",
    "winston": "^3.12.0",
    "winston-loki": "^6.0.8"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.2.2",
    "@types/express": "^4.17.21",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.11.27",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "eslint": "^8.57.0",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "tsc-alias": "^1.8.8",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.2"
  },
  "main": "dist/main.js",
  "scripts": {
    "postinstall": "patch-package",
    "slash-deploy": "node scripts/deploy-commands.js",
    "slash-delete": "node scripts/delete-commands.js",
    "migrate": "node scripts/migrate.js",
    "build": "rimraf dist/ && genversion --es6 --semi src/lib/util/version.ts && tsc && tsc-alias",
    "build-start": "npm run build && npm start",
    "start": "node .",
    "dev": "node --enable-source-maps .",
    "lint": "eslint . --ext .ts"
  },
  "imports": {
    "#src/*": "./src/*"
  },
  "release": {
    "tagFormat": "${version}",
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/npm",
        {
          "npmPublish": false
        }
      ],
      [
        "@semantic-release/git",
        {
          "assets": [
            "package.json",
            "package-lock.json"
          ],
          "message": "chore(release): ${nextRelease.version}\n\n${nextRelease.notes}"
        }
      ],
      "@semantic-release/github"
    ]
  }
}
