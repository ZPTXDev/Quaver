{
  "name": "quaver",
  "version": "6.8.1-next.1",
  "type": "module",
  "engines": {
    "node": ">=16.9.0"
  },
  "dependencies": {
    "@discordjs/rest": "^1.5.0",
    "@jeve/lyrics-finder": "^1.0.1",
    "@keyv/sqlite": "^3.6.4",
    "@lavaclient/queue": "^2.1.1",
    "@lavaclient/spotify": "^3.1.0",
    "@napi-rs/pinyin": "^1.7.1",
    "@types/express": "^4.17.15",
    "crypto-js": "^4.1.1",
    "discord-api-types": "^0.37.24",
    "discord.js": "^14.7.1",
    "express": "^4.18.2",
    "genversion": "^3.1.1",
    "hangul-romanization": "^1.0.1",
    "jsonwebtoken": "^9.0.0",
    "keyv": "^4.5.2",
    "kuroshiro": "1.1.2",
    "kuroshiro-analyzer-kuromoji": "^1.1.0",
    "lavaclient": "^4.1.1",
    "lodash-es": "^4.17.21",
    "patch-package": "^6.5.0",
    "socket.io": "^4.5.4",
    "wanakana": "^5.0.2",
    "winston": "^3.8.2",
    "winston-loki": "^6.0.6"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.1.1",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^18.11.17",
    "@types/wanakana": "^4.0.3",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "@typescript-eslint/parser": "^5.47.0",
    "eslint": "^8.30.0",
    "rimraf": "^3.0.2",
    "tsc-alias": "^1.8.2",
    "tsconfig-paths": "^4.1.1",
    "typescript": "^4.9.4"
  },
  "main": "dist/main.js",
  "scripts": {
    "postinstall": "patch-package",
    "slash-deploy": "node scripts/deploy-commands.js",
    "slash-delete": "node scripts/delete-commands.js",
    "migrate": "node scripts/migrate.js",
    "build": "rimraf dist/ && genversion --es6 --semi src/lib/util/version.ts && tsc && tsc-alias",
    "build-start": "npm run build && npm start",
    "start": "node .",
    "dev": "node --enable-source-maps .",
    "lint": "eslint . --ext .ts"
  },
  "imports": {
    "#src/*": "./src/*"
  },
  "release": {
    "tagFormat": "${version}",
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/npm",
        {
          "npmPublish": false
        }
      ],
      [
        "@semantic-release/git",
        {
          "assets": [
            "package.json",
            "package-lock.json"
          ],
          "message": "chore(release): ${nextRelease.version}\n\n${nextRelease.notes}"
        }
      ],
      "@semantic-release/github"
    ]
  }
}
